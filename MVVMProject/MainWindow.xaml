<Window x:Class="MVVMProject.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MVVMProject"
        xmlns:views="clr-namespace:MVVMProject.Views"
        mc:Ignorable="d"
        xmlns:viewModel="clr-namespace:MVVMProject.ViewModel"
        Title="MainWindow" Height="450" Width="800"
        FontSize="15">
    <Grid Margin="20 10">
        

        <Grid.Resources>
            <DataTemplate DataType="{x:Type viewModel:ReservationListingViewModel}">
                <views:ReservationListingView/>
            </DataTemplate>
            <DataTemplate DataType="{x:Type viewModel:MakeReservationViewModel}">
                <views:MakeReservationView/>
            </DataTemplate>
        </Grid.Resources>

        <ContentPresenter Grid.Column="0" Content="{Binding Navigation.CurrentViewModel}" />
    </Grid>
</Window>
