<Application x:Class="MVVMProject.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MVVMProject" 
             xmlns:views="clr-namespace:MVVMProject.Views"
             xmlns:viewModel="clr-namespace:MVVMProject.ViewModel" 
             >
    <Application.Resources>


            <DataTemplate DataType="{x:Type viewModel:ReservationListingViewModel}">
                <views:ReservationListingView/>
            </DataTemplate>
            <DataTemplate DataType="{x:Type viewModel:MakeReservationViewModel}">
                <views:MakeReservationView/>
            </DataTemplate>
        
        

    </Application.Resources>
</Application>


